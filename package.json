{
	"$schema": "http://json.schemastore.org/package",
	"name": "disclosure-bot",
	"version": "0.1.1",
	"private": true,
	"main": "./src/index.ts",
	"scripts": {
		"start": "ts-node .",
		"update": "ts-node src/scripts/update.ts",
		"env": "ts-node src/scripts/env.ts",
		"build": "tsc --noEmit",
		"lint": "prettier --check \"src/**/*.{ts,tsx}\"",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"v:patch": "npm version patch -m \"chore(release): %s\"",
		"v:minor": "npm version minor -m \"chore(release): %s\""
	},
	"dependencies": {
		"@discordjs/builders": "^0.12.0",
		"@discordjs/rest": "^0.3.0",
		"@oadpoaw/utils": "^0.2.4",
		"@sapphire/ts-config": "^3.3.2",
		"@types/inquirer": "^8.2.0",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.21",
		"@types/sha256-file": "^1.0.1",
		"discord-api-types": "^0.28.0",
		"discord.js": "^13.6.0",
		"inquirer": "^8.2.0",
		"ms": "^2.1.3",
		"node-fetch": "^2.6.1",
		"prettier": "^2.5.1",
		"sha256-file": "^1.0.0",
		"ts-node": "^10.7.0",
		"tsconfig-paths": "^3.13.0",
		"typescript": "^4.6.2",
		"winston": "^3.6.0",
		"winston-daily-rotate-file": "^4.6.1",
		"yaml": "^1.10.2"
	},
	"engines": {
		"node": ">=v16.0.0",
		"npm": ">=7.0.0"
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"tabWidth": 4,
		"semi": true,
		"endOfLine": "auto",
		"useTabs": true
	},
	"devDependencies": {
		"@types/node-fetch": "^2.6.1"
	}
}
