{
	"$schema": "http://json.schemastore.org/package",
	"name": "disclosure-bot",
	"version": "1.1.0",
	"private": true,
	"main": "./dist/index.js",
	"type": "module",
	"scripts": {
		"start": "node --no-warnings .",
		"predev": "npm run clean",
		"dev": "concurrent \"tsc -w\" \"nodemon .\"",
		"postdev": "npm run clean",
		"upgrade": "node dist/scripts/update.js",
		"env": "node dist/scripts/env.js",
		"prebuild": "npm run clean",
		"build": "tsc --build",
		"postbuild": "node scripts/postbuild.js",
		"clean": "rimraf dist && rimraf build",
		"lint": "prettier --check \"src/**/*.ts\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"prebuild:plugins": "node scripts/prebuild-plugins.js",
		"build:plugins": "tsc --project plugin.tsconfig.json",
		"postbuild:plugins": "node scripts/postbuild-plugins.js",
		"v:patch": "npm version patch -m \"chore(release): %s\"",
		"v:minor": "npm version minor -m \"chore(release): %s\"",
		"v:major": "npm version major -m \"chore(release): %s\""
	},
	"dependencies": {
		"@discordjs/builders": "^0.12.0",
		"@discordjs/rest": "^0.3.0",
		"@oadpoaw/utils": "^0.2.4",
		"discord-api-types": "^0.29.0",
		"discord.js": "^13.6.0",
		"inquirer": "^8.2.1",
		"ms": "^2.1.3",
		"node-fetch": "^3.2.2",
		"package-name-regex": "^2.0.6",
		"semver-regex": "^4.0.2",
		"sha256-file": "^1.0.0",
		"ts-node": "^10.7.0",
		"winston": "^3.6.0",
		"winston-daily-rotate-file": "^4.6.1",
		"yaml": "^1.10.2"
	},
	"devDependencies": {
		"@types/inquirer": "^8.2.0",
		"@types/keyv": "^3.1.3",
		"@types/module-alias": "^2.0.1",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.21",
		"@types/node-fetch": "^2.6.1",
		"@types/package-name-regex": "^2.0.0",
		"@types/sha256-file": "^1.0.1",
		"concurrently": "^7.0.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.6.2"
	},
	"engines": {
		"node": ">=v17.0.0",
		"npm": ">=8.0.0"
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"tabWidth": 4,
		"semi": true,
		"endOfLine": "auto",
		"useTabs": true
	},
	"imports": {
		"#disclosure/*": "./dist/structures/*.js"
	}
}
