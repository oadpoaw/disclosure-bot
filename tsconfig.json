{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@oadpoaw/ts-config",
	"compilerOptions": {
		"outDir": "build",
		"paths": {
			"#disclosure/*": [
				"structures/*"
			]
		}
	},
	"include": [
		"src"
	],
	"exclude": [],
	"typedocOptions": {
		"out": "./docs/",
		"readme": "./README.md",
		"name": "disclosure-bot",
		"entryPoints": [
			"src/classes/Client.ts",
			"src/structures/Plugin.ts",
			"src/structures/PluginMetaData.ts"
		],
		"excludeExternals": true,
		"excludeInternal": false,
		"excludePrivate": true
	}
}