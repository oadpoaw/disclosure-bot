{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"target": "ESNext",
		"composite": false,
		"declaration": false,
		"declarationMap": false,
		"rootDir": "src",
		"outDir": "dist",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true
	},
	"include": [
		"src"
	]
}